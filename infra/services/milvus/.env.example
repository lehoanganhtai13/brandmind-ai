# Milvus Configuration
# Copy this file to .env and update with your actual values
# IMPORTANT: Add .env to .gitignore to avoid committing secrets

# Milvus Server Ports
MILVUS_PORT=19530
MILVUS_METRICS_PORT=9091

# Attu UI Port
ATTU_PORT=3001

# MinIO Configuration (shared by MinIO service and Milvus)
# IMPORTANT: These credentials must match between MinIO and Milvus
MINIO_ACCESS_KEY_ID=minioadmin
MINIO_SECRET_ACCESS_KEY=minioadmin_password_change_me

# Milvus Authentication
# Set to false to disable authentication
MILVUS_AUTH_ENABLED=true
# Default root password (change this in production!)
MILVUS_ROOT_PASSWORD=Milvus
